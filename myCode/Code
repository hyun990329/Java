public class TotalGugudan {
    public static void main(String[] args) {
        // for 구문을 중첩해서 구구단을 출력하시오
        // 단을 회전하는 for ~ loop
        for (int i = 2; i <= 9; i++) {
            System.out.println("===" + i + "단" + "===");
            for (int j = 1; j <= 9; j++) {
                System.out.println(i + " * " + j + " = " + (i * j) );
            }
        }











    }
}
